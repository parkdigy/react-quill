{
  "name": "examples",
  "scripts": {
    "dev": "cross-env NODE_ENV=development LIB_ENV=development webpack serve --config=webpack.config.js",
    "dev-prd-lib": "cross-env NODE_ENV=development LIB_ENV=production webpack serve --config=webpack.config.js",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "clean:prod": "rm -rf build && rm -rf dist && mkdir dist",
    "clean:prod:win": "rmdir /s /q build && rmdir /s /q dist && mkdir dist",
    "copy:index": "cp ./dist/index.html ../index.html && cp ./dist/index.html ../404.html",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress --config webpack.config.js",
    "build": "npm run clean:prod && npm run lint && npm run build:webpack && npm run copy:index",
    "reinstall-module": "rm -rf node_modules && rm -f package-lock.json && npm i"
  },
  "devDependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.13",
    "@mui/material": "^5.15.13",
    "@pdg/react-admin-layout": "^1.0.29",
    "@pdg/react-hook": "^1.0.16",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.27",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "quill": "^2.0.0-rc.5",
    "react-app-rewired": "^2.2.1",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1",
    "sass": "^1.72.0",
    "sass-loader": "^14.1.1",
    "simplebar-react": "3.2.4",
    "source-map-loader": "^5.0.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-shell-plugin-next": "^2.3.1"
  }
}
